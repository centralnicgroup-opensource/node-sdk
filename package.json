{
  "name": "ispapi-apiconnector",
  "description": "1API API Connector for node.js",
  "version": "4.1.1",
  "author": {
    "name": "Kai Schwarz",
    "email": "kschwarz@1api.net"
  },
  "main": "../dist/index.js",
  "license": "MIT",
  "engines": {
    "node": ">=0.10.36"
  },
  "homepage": "http://1api.net",
  "maintainers": [
    {
      "name": "Kai Schwarz",
      "email": "kschwarz@1api.net"
    }
  ],
  "keywords": [
    "1api",
    "domain",
    "api",
    "connector",
    "isp",
    "ispapi",
    "ssl",
    "cert",
    "dns",
    "backorder",
    "premium",
    "preregistration",
    "registration",
    "application"
  ],
  "nyc": {
    "reporter": [
      "html",
      "text"
    ],
    "exclude": [
      "**/*.spec.js",
      "src/test-check-helper.js",
      "src/test-commands.js"
    ]
  },
  "scripts": {
    "build": "tsc --version && tsc && npm run lint --silent",
    "changelog": "auto-changelog --package --output HISTORY.md; git add HISTORY.md;",
    "documentation": "npm run docco --silent && npm run typedoc --silent",
    "docco": "docco -l parallel -o docs/docco src/index.ts",
    "lint": "eslint --fix --config=./.eslintrc.js *.js README.md *.json dist/*.js src/*.spec.js",
    "test": "npm run build && npm run test-NODE --silent",
    "test-NODE": "cross-env NODE_ENV=development nyc ./node_modules/.bin/_mocha --opts mocha.opts $(find . -type f -name '*.spec.js'  ! -path './nyc_output/*' ! -path './coverage/*' ! -path './node_modules/*')",
    "typedoc": "typedoc --out docs/typedoc --name ispapi-entitydetermination --readme ./README.md --mode file --includeDeclarations --excludeExternals src/index.ts",
    "preversion": "git fetch origin --prune && git checkout master && git pull && npm test",
    "version": "npm run changelog --silent",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/node": "^9.6.0",
    "auto-changelog": "0.3.5",
    "chai": "4.1.2",
    "cross-env": "5.0.5",
    "istanbul": "0.4.5",
    "mocha": "4.0.1",
    "nock": "9.0.22",
    "nyc": "11.2.1"
  },
  "dependencies": {}
}
