{
  "name": "@hexonet/ispapi-apiconnector",
  "description": "Node.js SDK for the insanely fast HEXONET API",
  "version": "9.0.1",
  "private": false,
  "author": {
    "name": "Kai Schwarz",
    "email": "kschwarz@hexonet.net"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "//": "...",
  "license": "MIT",
  "engines": {
    "node": ">=20.6.1"
  },
  "homepage": "https://github.com/centralnicgroup-opensource/rtldev-middleware-node-sdk",
  "repository": "github:centralnicgroup-opensource/rtldev-middleware-node-sdk",
  "bugs": "https://github.com/centralnicgroup-opensource/rtldev-middleware-node-sdk/issues",
  "maintainers": [
    {
      "name": "Kai Schwarz",
      "email": "kschwarz@hexonet.net"
    }
  ],
  "keywords": [
    "hexonet",
    "domain",
    "api",
    "connector",
    "isp",
    "ispapi",
    "ssl",
    "cert",
    "dns",
    "backorder",
    "premium",
    "preregistration",
    "registration",
    "application"
  ],
  "nyc": {
    "reporter": [
      "html",
      "text"
    ],
    "exclude": [
      "**/*.spec.ts",
      "**/*.spec.js"
    ]
  },
  "scripts": {
    "compile": "tsc --version && tsc --declaration",
    "lint": "prettier --write .",
    "prepublish": "npm run compile",
    "documentation": "npm run gendocsDocco --silent && npm run gendocsTypedoc --silent",
    "gendocsTypedoc": "typedoc --out docs/typedoc --name @hexonet/ispapi-apiconnector --readme ./README.md --excludeExternals src/*.ts",
    "gendocsDocco": "docco -l parallel -o docs/docco src/*.ts",
    "test": "npm run compile && cross-env NODE_ENV=development nyc mocha"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^10.0.0",
    "@types/node": "^20.0.0",
    "chai": "^5.0.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.3",
    "docco": "^0.9.1",
    "gulp": "^4.0.2",
    "jsdoc": "^4.0.0",
    "jsdoc-oblivion": "^0.0.9",
    "mocha": "^10.0.0",
    "nock": "^13.2.7",
    "nyc": "^15.1.0",
    "prettier": "^3.0.0",
    "rtldev-middleware-gulp-release-notification-plugin": "github:centralnicgroup-opensource/rtldev-middleware-gulp-release-notification-plugin#semver:^1.0.0",
    "semantic-release": "^22.0.5",
    "supertest": "^6.2.3",
    "ts-node": "^10.8.1",
    "typedoc": "^0.25.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "cross-fetch": "^4.0.0"
  },
  "overrides": {
    "glob-parent": "^5.1.2",
    "chokidar": "^3.5.3"
  }
}
